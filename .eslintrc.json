{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "next",
    // "plugin:@typescript-eslint/eslint-recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "semi": ["error", "always"],
    "camelcase": "warn",
    "quotes": [2, "double"],
    "no-multiple-empty-lines": ["error", { "max": 1 }]
  }
  // "globals": {
  //     "google": "readonly"
  // }
}
